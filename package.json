{
  "name": "dns-playground",
  "version": "0.0.0-semantic-release",
  "sideEffects": false,
  "type": "module",
  "main": "./index.mjs",
  "exports": {
    ".": "./index.mjs"
  },
  "description": "playing around with dns & Symatem",
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme ./index.mjs --section=API",
    "lint": "npm run lint:docs",
    "lint:docs": "documentation lint ./index.mjs"
  },
  "dependencies": {
    "@symatem/core": "^0.0.8",
    "@symatem/ontology": "git+https://github.com/arlac77/SymatemOntology.git",
    "@symatem/query": "^3.1.11",
    "dnsz": "^1.3.0"
  },
  "devDependencies": {
    "ava": "^3.13.0",
    "c8": "^7.3.4",
    "documentation": "^13.0.2",
    "semantic-release": "^17.2.1"
  },
  "engines": {
    "node": ">=14.13.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/dns-playground.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/dns-playground/issues"
  },
  "homepage": "https://github.com/arlac77/dns-playground#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-esm-only",
      "arlac77/template-node-component"
    ]
  }
}
