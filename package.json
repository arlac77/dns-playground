{
  "name": "dns-playground",
  "version": "0.0.0-semantic-release",
  "sideEffects": false,
  "type": "module",
  "main": "{{module}}",
  "description": "playing around with dns & Symatem",
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "ava --timeout 2m",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme index.mjs --section=API && documentation readme {{module}} --section=API",
    "lint": "documentation lint index.mjs && documentation lint {{module}}"
  },
  "dependencies": {
    "SymatemJS": "git+https://github.com/Symatem/SymatemJS.git",
    "dnsz": "^1.1.5"
  },
  "devDependencies": {
    "ava": "^3.8.0",
    "c8": "^7.1.0",
    "documentation": "^12.3.0",
    "semantic-release": "^17.0.7"
  },
  "engines": {
    "node": ">=13.13.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/dns-playground.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/dns-playground/issues"
  },
  "homepage": "https://github.com/arlac77/dns-playground#readme",
  "template": {
    "inheritFrom": "arlac77/template-esm-only"
  }
}
